.data
	texto: 	.asciiz "sum = "
	texto1: .asciiz "\n"
.text
	# $t0 = upTo / $t1 = i / $t2 = sum
	main:
		li $v0, 5	# carrega o código de ler inteiro
		syscall 
		
		move $t0,$v0	# valor fornecido está em $t0
		
		li $t2,0	# sum = 0
		li $t1,0	# i = 0
	for:
		beq $t1,$t0,fimFor 	# Se i == upTo vai para fimFor,se não continua
		mul $t3,$t1,$t1		# i = i * i
		add $t2,$t2,$t3		# sum = sum + $t3
		addi $t1,$t1,1		# i = i + 1
		j for
	fimFor:
		li $v0, 4		# carrega o código de imprimir string
		la $a0, texto		# carrega o endereço da string a ser impressa em a0
		syscall			# chama o SO para realizar a operação
		
		li $v0,1		# carrega o código de imprimir inteiro
		move $a0, $t2		# move o inteiro para a0
		syscall 		# chama o SO para realizar a operação
		
		li $v0, 4		# carrega o código de imprimir string
		la $a0, texto1		# carrega o endereço da string a ser impressa em a0
		syscall			# chama o SO para realizar a operação
		
		li $v0, 10 		# carrega o código da operação para terminar o programa
		syscall			# chama o SO para realizar a operação
